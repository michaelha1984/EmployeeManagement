@model int

@{ 
    ViewBag.Title = "404 Error";
}

<div class="alert alert-danger mb-1">
    <h4>404 Not Found Error : </h4>
    <hr />
    <h5>
        Employee with Id = @Model can not be found 
    </h5>
</div>
<a asp-action="index" asp-controller="home" class="btn btn-outline-success" style="width: auto;">
    Click here to see all employees.
</a>